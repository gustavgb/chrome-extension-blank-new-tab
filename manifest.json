{
  "name": "Blank new tab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Blank new tab",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://*/"
  ],
  "chrome_url_overrides": {
    "newtab": "src/override/dist/newtab.html"
  },
  "content_security_policy": "script-src 'self'"
}
